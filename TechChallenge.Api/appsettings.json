{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "${{ secrets.JWT_KEY }}",
    "Issuer": "ApiContatos"
  },
  "ConnectionStrings": {
    "PostgreSQL": "${{ secrets.ORL }};Port=6543;Username=${{ secrets.DB_LOGIN }};Password=${{ secrets.DB_PASSWORD }};Database=postgres;Pooling=false;"
  }
}
